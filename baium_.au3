EndFunc

Func _datenextmonthnum($imonthnum)
	Select 
		Case NOT StringIsInt($imonthnum)
			Return SetError(1, 0, 0)
		Case NOT __dateismonth($imonthnum)
			Return SetError(2, 0, 0)
		Case Else
			Local $inextmonthnum
			If $imonthnum = 12 Then
				$inextmonthnum = 1
			Else
